  <DataSets>
    <DataSet Name="Main">
      <Query>
        <DataSourceName>CurriculamDB</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PathName">
            <Value>=Parameters!PathName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CourseName">
            <Value>=Parameters!CourseName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SectionName">
            <Value>=Parameters!SectionName.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EventName">
            <Value>=Parameters!EventName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT P.PathID, P.PathName, P.PathDesc, P.PathActive, C.CourseID, C.CourseName, C.CourseDesc, C.CourseActive, S.SectionID, S.SectionName, S.SectionDesc, S.SectionActive, E.EventID, ISNULL(E.EventName, 'No Name') AS EventName, E.EventActive
FROM   ADF_Event AS E LEFT OUTER JOIN
             ADF_Section AS S ON E.SectionID = S.SectionID LEFT OUTER JOIN
             ADF_Course AS C ON C.CourseID = S.CourseID LEFT OUTER JOIN
             ADF_Path AS P ON P.PathID = C.PathID
WHERE (P.PathID IS NOT NULL)
And       P.PathID in (@PathName)
And       C.CourseID in (@CourseName)
And    S.SectionID in (@SectionName)
And    E.EventID in (@EventName)
ORDER BY P.PathID, C.CourseID, S.SectionID, E.EventID</CommandText>
      </Query>
